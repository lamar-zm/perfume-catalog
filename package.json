{
  "name": "perfume-catalog-monorepo",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "npm run dev:web",
    "dev:web": "npm run dev --workspace=apps/web",
    "build": "npm run build --workspace=apps/web",
    "build:web": "npm run build --workspace=apps/web",
    "start": "npm run start --workspace=apps/web",
    "lint": "npm run lint --workspace=apps/web",
    "db:seed": "npm run db:seed --workspace=packages/database",
    "db:reset": "npm run db:reset --workspace=packages/database"
  },
  "devDependencies": {
    "typescript": "^5.0.0"
  }
}
